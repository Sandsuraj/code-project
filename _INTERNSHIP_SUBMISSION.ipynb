      },
      "source": [
        "import whisper\n",
        "import librosa\n",
        "import nltk\n",
        "import torch\n",
        "import re\n",
        "from transformers import pipeline\n",
        "from nltk.tokenize import sent_tokenize"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "e83a82a1"
      },
      "source": [
        "nltk.download('punkt')\n",
        "nltk.download('punkt_tab')"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "3030d9c5"
      },
      "source": [
        "def preprocess_audio(audio_path):\n",
        "    audio, sr = librosa.load(audio_path, sr=16000)\n",
        "    return audio"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "383f7f61"
      },
      "source": [
        "asr_model = whisper.load_model(\"base\")"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "6126d2ee"
      },
      "source": [
        "def transcribe_audio(audio_path):\n",
        "    result = asr_model.transcribe(audio_path)\n",
        "    return result[\"text\"]"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "c4164b3b"
      },
      "source": [
        "def clean_text(text):\n",
        "    text = text.lower()\n",
        "    text = re.sub(r'\\b(uh|um|ah|er|mm)\\b', '', text)\n",
        "    text = re.sub(r'\\s+', ' ', text)\n",
        "    return text.strip()"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "c069cc14"
      },
      "source": [
        "def chunk_text(text, max_words=500):\n",
        "    sentences = sent_tokenize(text)\n",
        "    chunks = []\n",
        "    current_chunk = []\n",
        "\n",
        "    word_count = 0\n",
        "    for sentence in sentences:\n",
        "        words = sentence.split()\n",
        "        word_count += len(words)\n",
        "        current_chunk.append(sentence)\n",
        "\n",
        "        if word_count >= max_words:\n",
        "            chunks.append(\" \".join(current_chunk))\n",
        "            current_chunk = []\n",
        "            word_count = 0\n",
        "\n",
        "    if current_chunk:\n",
        "        chunks.append(\" \".join(current_chunk))\n",
        "\n",
        "    return chunks"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "c8df4aa1",
        "outputId": "1d37e1fb-3d1e-4644-a509-9de5e291b655"
      },
      "source": [
        "summarizer = pipeline(\n",
        "    \"summarization\",\n",
        "    model=\"facebook/bart-large-cnn\",\n",
        "    device=0 if torch.cuda.is_available() else -1\n",
        ")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stderr",
          "text": [
            "Device set to use cpu\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "b5f45a26"
      },
      "source": [
        "def summarize_chunks(chunks):\n",
        "    summaries = []\n",
        "\n",
        "    for chunk in chunks:\n",
        "        summary = summarizer(\n",
        "            chunk,\n",
        "            max_length=150,\n",
        "            min_length=50,\n",
        "            do_sample=False\n",
        "        )[0]['summary_text']\n",
        "\n",
        "        summaries.append(summary)\n",
        "\n",
        "    return summaries"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "519ab67c"
      },
      "source": [
        "def final_summary(chunk_summaries):\n",
        "    combined_text = \" \".join(chunk_summaries)\n",
        "\n",
        "    final = summarizer(\n",
        "        combined_text,\n",
        "        max_length=200,\n",
        "        min_length=80,\n",
        "        do_sample=False\n",
        "    )[0]['summary_text']\n",
        "\n",
        "    return final"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "da97cb86"
      },
      "source": [
        "def podcast_summarizer(audio_path):\n",
        "    print(\"ðŸ”Š Transcribing audio...\")\n",
        "    transcript = transcribe_audio(audio_path)\n",
        "\n",
        "    print(\"ðŸ§¹ Cleaning transcript...\")\n",
        "    clean_transcript = clean_text(transcript)\n",
        "\n",
        "    print(\"âœ‚ï¸ Chunking text...\")\n",
        "    chunks = chunk_text(clean_transcript)\n",
        "\n",
        "    print(f\"ðŸ“¦ Total Chunks: {len(chunks)}\")\n",
        "\n",
        "    print(\"ðŸ§  Summarizing chunks...\")\n",
        "    chunk_summaries = summarize_chunks(chunks)\n",
        "\n",
        "    print(\"âœ¨ Creating final summary...\")\n",
        "    final = final_summary(chunk_summaries)\n",
        "\n",
        "    return {\n",
        "        \"transcript\": clean_transcript,\n",
        "        \"chunk_summaries\": chunk_summaries,\n",
        "        \"final_summary\": final\n",
        "    }"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "389d7cfe"
      },
      "source": [
        "# Download a sample audio file\n",
        "!wget -O sample_audio.mp3 https://www.soundhelix.com/examples/mp3/SoundHelix-Song-1.mp3\n",
        "\n",
        "audio_file_path = 'sample_audio.mp3'\n",
        "\n",
        "print(f\"Using audio file: {audio_file_path}\")"
