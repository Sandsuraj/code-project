      "source": [
        "def summarize_chunks(chunks):\n",
        "    summaries = []\n",
        "\n",
        "    for chunk in chunks:\n",
        "        summary = summarizer(\n",
        "            chunk,\n",
        "            max_length=150,\n",
        "            min_length=50,\n",
        "            do_sample=False\n",
        "        )[0]['summary_text']\n",
        "\n",
        "        summaries.append(summary)\n",
        "\n",
        "    return summaries\n"
      ],
      "metadata": {
        "id": "KRrtkBNW3z_f"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def final_summary(chunk_summaries):\n",
        "    combined_text = \" \".join(chunk_summaries)\n",
        "\n",
        "    final = summarizer(\n",
        "        combined_text,\n",
        "        max_length=200,\n",
        "        min_length=80,\n",
        "        do_sample=False\n",
        "    )[0]['summary_text']\n",
        "\n",
        "    return final\n"
      ],
      "metadata": {
        "id": "ug5-PdES3zxO"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def podcast_summarizer(audio_path):\n",
        "    print(\"ðŸ”Š Transcribing audio...\")\n",
        "    transcript = transcribe_audio(audio_path)\n",
        "\n",
        "    print(\"ðŸ§¹ Cleaning transcript...\")\n",
        "    clean_transcript = clean_text(transcript)\n",
        "\n",
        "    print(\"âœ‚ï¸ Chunking text...\")\n",
        "    chunks = chunk_text(clean_transcript)\n",
        "\n",
        "    print(f\"ðŸ“¦ Total Chunks: {len(chunks)}\")\n",
        "\n",
        "    print(\"ðŸ§  Summarizing chunks...\")\n",
        "    chunk_summaries = summarize_chunks(chunks)\n",
        "\n",
        "    print(\"âœ¨ Creating final summary...\")\n",
        "    final = final_summary(chunk_summaries)\n",
        "\n",
        "    return {\n",
        "        \"transcript\": clean_transcript,\n",
        "        \"chunk_summaries\": chunk_summaries,\n",
        "        \"final_summary\": final\n",
        "    }\n"
      ],
      "metadata": {
        "id": "G6JE4Qyi4AKi"
      },
      "execution_count": null,
      "outputs": []
    },
