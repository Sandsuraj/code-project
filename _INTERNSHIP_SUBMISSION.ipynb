        "id": "0b22a6d8"
      },
      "source": [
        "# Call the podcast_summarizer function again, now that NLTK resources are ready\n",
        "summary_output = podcast_summarizer(audio_file_path)\n",
        "\n",
        "print(\"\\nFINAL PODCAST SUMMARY:\")\n",
        "print(summary_output[\"final_summary\"])"
      ],
      "execution_count": null,
      "outputs": []
    },
